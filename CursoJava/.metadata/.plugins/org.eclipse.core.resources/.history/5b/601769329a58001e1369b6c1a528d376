package com.giovani.operadores;

public class Operadores {

	public static void main(String[] args) {

		String nome = "Giovani";
		double soma = 10.5 + 15.7 ;
		System.out.println(soma);
		int subtracao = 10 - 5 ;
		System.out.println(subtracao);
		int modulo = 5 % 2;
		System.out.println(modulo);

	}

}
