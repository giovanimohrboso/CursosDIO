package com.giovani.tipos;

public class TiposBasicos {

	public static void main(String[] args) {
		
		byte idade = 1; //tamanho 128
		System.out.println(idade);
		short ano = 2023; //32768
		System.out.println(ano);
		int saldo = 10000; //2.147.483.648
		System.out.println(saldo);
		long estrelas = 1000000000l; //quase infinito
		System.out.println(estrelas);
		
		float valor1 = 1.3f;
		System.out.println(valor1);
		double valor2 = 124.5;
		System.out.println(valor2);
		
		
		

	}

}
